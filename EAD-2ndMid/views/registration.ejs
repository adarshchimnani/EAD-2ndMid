<%- include("header") -%>

  <div class="box" id="heading">
    <h1>
      <%= listTitle %>
    </h1>
  </div>

  <div class="box">
    <form class="item" action="/registeredUsers" method="post">
      <input type="file" id="myFile" name="filename"><br>
      <input type="text" name="name" placeholder="Full Name" required><br>
      <input type="email" name="email" placeholder="Email" required><br>
      <input type="tel" name="number" placeholder="Number" required><br>
      <select id="country" name="country"><br>
        <option>Select Country</option>
        <option value="NG">Nigeria</option>
        <option value="NU">Niue</option>
        <option value="NF">Norfolk Island</option>
        <option value="MP">Northern Mariana Islands</option>
        <option value="NO">Norway</option>
        <option value="OM">Oman</option>
        <option value="PK">Pakistan</option>
      </select><br>
      <input type="text" name="state" placeholder="State/Region" required><br>
      <input type="text" name="city" placeholder="City" required><br>
      <input type="text" name="address" placeholder="Address" required><br>
      <input type="text" name="zipCode" placeholder="Zip Code" required><br>
      <button type="submit" name="list">Register</button><br>
    </form>
    


  </div>

  <%- include("footer") -%>